# Cache static assets aggressively (for Netlify, Vercel, etc.)
/packs/*
  Cache-Control: public, max-age=31536000, immutable
  
/assets/*
  Cache-Control: public, max-age=31536000, immutable

# PMTiles files - enable range requests and caching
/packs/*.pmtiles
  Cache-Control: public, max-age=31536000, immutable
  Accept-Ranges: bytes
  
# CSV and binary data files
/packs/*.csv
  Cache-Control: public, max-age=31536000, immutable
  
/packs/*.bin
  Cache-Control: public, max-age=31536000, immutable

# WASM files
*.wasm
  Cache-Control: public, max-age=31536000, immutable
  Content-Type: application/wasm
